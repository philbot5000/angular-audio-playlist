8341 silly resolved     homepage: 'https://github.com/rmustacc/node-ctype',
8341 silly resolved     author: { name: 'Robert Mustacchi', email: 'rm@fingolfin.org' },
8341 silly resolved     engines: { node: '>= 0.4' },
